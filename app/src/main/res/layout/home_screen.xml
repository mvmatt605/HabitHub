<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeScreen">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#698fa8"
        android:backgroundTint="#698fa8">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="48dp"
            android:layout_height="65dp"
            android:adjustViewBounds="false"
            android:elevation="10dp"
            android:focusable="auto"
            android:foreground="@drawable/arrow"
            android:visibility="visible"
            app:srcCompat="@drawable/arrow"
            tools:layout_editor_absoluteX="525dp"
            tools:layout_editor_absoluteY="369dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="143dp"
            android:layout_height="154dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintHorizontal_bias="0.59"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo" />

        <Button
            android:id="@+id/NutritionScreenButton"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:backgroundTint="#a0dfce"
            android:text="Nutrition"
            android:textColor="#698fa8"
            android:textSize="34sp"
            app:layout_constraintBottom_toTopOf="@+id/habitScreenButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/expenseScreenButton" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="48dp"
            android:layout_height="65dp"
            android:adjustViewBounds="false"
            android:elevation="10dp"
            android:focusable="auto"
            android:foreground="@drawable/arrow"
            android:visibility="visible"
            app:srcCompat="@drawable/arrow"
            tools:layout_editor_absoluteX="525dp"
            tools:layout_editor_absoluteY="573dp" />

        <Button
            android:id="@+id/expenseScreenButton"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_marginTop="108dp"
            android:layout_weight="1"
            android:backgroundTint="#a0dfce"
            android:text="Expenses"
            android:textColor="#698fa8"
            android:textSize="34sp"
            app:layout_constraintBottom_toTopOf="@+id/NutritionScreenButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="48dp"
            android:layout_height="65dp"
            android:adjustViewBounds="false"
            android:elevation="10dp"
            android:focusable="auto"
            android:foreground="@drawable/arrow"
            android:visibility="visible"
            app:srcCompat="@drawable/arrow"
            tools:layout_editor_absoluteX="524dp"
            tools:layout_editor_absoluteY="669dp" />

        <Button
            android:id="@+id/FitnessScreenButton"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_marginBottom="150dp"
            android:layout_weight="1"
            android:backgroundTint="#a0dfce"
            android:text="Fitness"
            android:textColor="#698fa8"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/habitScreenButton" />

        <Button
            android:id="@+id/habitScreenButton"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:backgroundTint="#a0dfce"
            android:text="habits"
            android:textColor="#698fa8"
            android:textSize="34sp"
            app:layout_constraintBottom_toTopOf="@+id/FitnessScreenButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/NutritionScreenButton" />

        <TextView
            android:id="@+id/trackTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/calistoga"
            android:text="Track"
            android:textColor="#a0dfce"
            android:textSize="48sp"
            app:layout_constraintBottom_toTopOf="@+id/expenseScreenButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="48dp"
            android:layout_height="65dp"
            android:adjustViewBounds="false"
            android:elevation="10dp"
            android:focusable="auto"
            android:foreground="@drawable/arrow"
            android:visibility="visible"
            app:srcCompat="@drawable/arrow"
            tools:layout_editor_absoluteX="525dp"
            tools:layout_editor_absoluteY="475dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>





</LinearLayout>